pipeline {
    agent any
    environment{
        PACKAGE_NAME = ""
    }
    stages {
        
        stage('pull code.....') {
            steps {
                echo '${p1}'
                }
        }
        stage('build code.....') {
            steps {
                echo '${p2}'
            }
        }
        stage('package code.....') {
            steps {
                echo '${p3}'
            }
        }
        stage('deploy package.....') {
            steps {
               echo '${p4}'
            }
        }
        
    }
}
